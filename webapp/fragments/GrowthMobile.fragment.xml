<core:FragmentDefinition xmlns:core="sap.ui.core" xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m" xmlns:l="sap.ui.layout"
	xmlns:f="sap.ui.layout.form">
	<l:Grid defaultSpan="L12 M12 S12" width="auto">
		<l:content>
			<f:SimpleForm minWidth="1024" maxContainerCols="2" editable="false" layout="ResponsiveGridLayout" labelSpanL="4" labelSpanM="4"
				emptySpanL="1" emptySpanM="1" columnsL="1" columnsM="1">
				<f:content>
					<Label text="Land Area" design="Bold"/>
					<Text text="{LandArea}"/>
					<Label text="Project Gross Value" design="Bold"/>
					<Text text="{ProjectGrossValue}"/>
					<Label text="Less: Estimated Liabilities taken over" design="Bold"/>
					<Text text="{EstdLiabilities}"/>
					<Label text="Net Consideration" design="Bold"/>
					<Text text="{NetConsideration}"/>
					<Label text="On Signing" design="Bold"/>
					<Text text="{OnSigning}"/>
					<Label text="On Plan Sanction" design="Bold"/>
					<Text text="{OnPlanSanction}"/>
					<Label text="Other Milestone 1" design="Bold"/>
					<Text text="{OtherMilestone1}"/>
					<Label text="Other Milestone 2" design="Bold"/>
					<Text text="{OtherMilestone2}"/>
					<Label text="Transaction Cost" design="Bold"/>
					<Text text="{TransactionCost}"/>
					<Label text="Transaction Brokerage" design="Bold"/>
					<Text text="{TransactionBrokerage}"/>
					<Label text="Legal Consultant Fees &amp; others" design="Bold"/>
					<Text text="{LegalConsultantFees}"/>
					<Label text="Financial DD Consultant Fees &amp; others" design="Bold"/>
					<Text text="{FinancialConsultantFees}"/>
					<Label text="Stamp Duty Costs" design="Bold"/>
					<Text text="{StampDutyCost}"/>
					<Label text="Others" design="Bold"/>
					<Text text="{Others}"/>
					<Label text="Total" design="Bold"/>
					<Text text="{Total}"/>
				</f:content>
			</f:SimpleForm>
		</l:content>
	</l:Grid>
</core:FragmentDefinition>