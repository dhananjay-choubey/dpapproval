<core:FragmentDefinition xmlns:core="sap.ui.core" xmlns="sap.m" xmlns:l="sap.ui.layout" xmlns:f="sap.ui.layout.form">
	<l:Grid defaultSpan="L12 M12 S12" width="auto">
		<l:content>
			<f:SimpleForm minWidth="1024" maxContainerCols="2" editable="false" layout="ResponsiveGridLayout" labelSpanL="6" labelSpanM="6"
				emptySpanL="1" emptySpanM="1" columnsL="2" columnsM="2">
				<f:content>
					<core:Title/>
					<Label text="Pre-Construction Start Date"/>
					<Text text="{path: 'PreConstStartDate', type: 'sap.ui.model.type.Date', formatOptions: { UTC: true, style: 'short' }}"/>
					<core:Title/>
					<Label text="Pre-Construction End Date"/>
					<Text text="{path: 'PreConstEndDate', type: 'sap.ui.model.type.Date', formatOptions: { UTC: true, style: 'short' }}"/>
				</f:content>
			</f:SimpleForm>
		</l:content>
	</l:Grid>
</core:FragmentDefinition>