<core:FragmentDefinition xmlns:core="sap.ui.core" xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m" xmlns:table="sap.ui.table">
	<table:Table selectionMode="None" showNoData="true" fixedColumnCount="1" enableColumnFreeze="true" enableSelectAll="false"
		showColumnVisibilityMenu="false" enableCellFilter="false" enableColumnReordering="false" visibleRowCountMode="Fixed"
		rows="{path:'/GrowthSet', templateShareable:true}" enableBusyIndicator="true" fixedRowCount="0" alternateRowColors="true"
		fixedBottomRowCount="1" visibleRowCount="8" visible="true">
		<table:columns>
			<table:Column width="20rem" resizable="true" showFilterMenuEntry="false" showSortMenuEntry="false" autoResizable="true" hAlign="Begin">
				<Label text="{i18n>transactionType}" wrapping="true"/>
				<table:template>
					<ObjectIdentifier title="" text="{TransactionType}" titleActive="false" visible="true"/>
				</table:template>
			</table:Column>
			<table:Column width="10rem" resizable="true" showFilterMenuEntry="false" showSortMenuEntry="false" autoResizable="true" hAlign="End">
				<Label text="{i18n>values}" wrapping="true"/>
				<table:template>
					<Text text="{path: 'Value', type: 'sap.ui.model.type.Float', formatOptions: { maxFractionDigits: 2 }}" width="auto" maxLines="1"
						wrapping="false" textAlign="End" textDirection="Inherit" visible="true"/>
				</table:template>
			</table:Column>
		</table:columns>
	</table:Table>
</core:FragmentDefinition>